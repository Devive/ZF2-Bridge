{
    "name": "php-di/zf2-bridge",
    "type": "library",
    "description": "Integrates PHP-DI to Zend Framework 2",
    "homepage": "https://github.com/PHP-DI/ZF2-Bridge",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "mnapoli/php-di": "~4.0"
    },

    "require-dev": {
        "zendframework/zendframework": "2.4.*",
        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd" : "@stable",
        "phpunit/phpunit": "4.6.*"
    },

    "autoload": {
        "classmap": [
          "src/", "vendor/", "tests"
        ]
    }
}
